{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run type-check && npm run lint && npm run test && npm run test:e2e",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "npm run type-check && npm run lint && npm run test && npm run build",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "verify:firebase": "node scripts/verify-firebase-config.js",
    "verify:maps": "node scripts/verify-google-maps-config.js",
    "verify:google-signin": "node test/google-signin-verification.js",
    "verify:env": "npm run verify:firebase && npm run verify:maps && npm run verify:google-signin",
    "clear-cache": "node scripts/clear-cache.js",
    "fresh-start": "npm run clear-cache && npm install && npm run dev",
    "setup:env": "cp .env.example .env.local && echo 'Please edit .env.local with your actual API keys'",
    "clean": "rm -rf .next out dist",
    "clean:install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "precommit": "npm run lint:fix && npm run type-check",
    "prepare": "husky install || true",
    "dev:clear-rate-limits": "node scripts/clear-rate-limits.js"
  },
